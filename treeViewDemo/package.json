{
	"name": "treeViewDemo",
	"displayName": "treeView",
	"description": "tree view demos",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.46.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onView:id_1"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "treeViewDemo.helloWorld",
				"title": "Hello World"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id":"neal_Tree",
					"title": "测试向活动栏提供视图容器",
					"icon": "./media/dep.png"
				}
			]
		},
		"views": {
			"explorer":[
				{
					"id": "id_explorer",
					"name": "资源管理器里面"
				}
			],
			"neal_Tree":[
				{
					"id": "id_1",
					"name": "第一组"
				},
				{
					"id": "id_2",
					"name": "第二组"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"test-compile": "tsc -p ./",
		"test-watch": "tsc -watch -p ./",
		"pretest": "npm run test-compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.46.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"eslint": "^7.19.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0",
		"ts-loader": "^8.0.14",
		"webpack": "^5.19.0",
		"webpack-cli": "^4.4.0"
	}
}
